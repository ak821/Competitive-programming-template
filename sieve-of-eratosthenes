int spf[maxn+1]; // spf[i] gives the smallest prime factor of i

void SieveOfEratosthenes(int n) 
{
    for (int p=2; p*p<=n; p++) 
    { 
        if (!spf[p]) 
        {
            for (int i=p*p; i<=n; i += p) 
                spf[i] = p; 
        } 
    } 
}

vector<int> factorisation(int n){ // n<= maxn
  vector<int> factors;
  while(n){
    n /= spf[n];
    factors.pb(spf[n]);
  }
  return factors;
}
